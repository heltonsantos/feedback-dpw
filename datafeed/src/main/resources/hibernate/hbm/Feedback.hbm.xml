<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="com.br.datafeed.model.Feedback" table="feedback">
        
        <id name="id" type="integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        
       <property name="dataset_id" type="integer">
       		<column name="DATASET_ID" not-null="true" unique="true" />
       </property>
       
       <property name="avaliacao_media" type="double">
            <column name="AVALIACAO_MEDIA" default="0" not-null="true"/>
        </property>
        
		<bag name="avaliacao" table="avaliacao" fetch="join" inverse="true">
			<key column="FEEDBACK_ID" not-null="true"/>
			<one-to-many class="com.br.datafeed.model.Avaliacao"/>
		</bag>
		
    </class>
</hibernate-mapping>